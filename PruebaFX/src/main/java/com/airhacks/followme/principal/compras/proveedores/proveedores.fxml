<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.airhacks.followme.principal.compras.proveedores.ProveedoresPresenter">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="tablaProveedores" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </BorderPane.margin>
            </TableView>
         </center>
         <top>
            <HBox spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Actualizar" />
                  <Region prefHeight="25.0" prefWidth="288.0" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="filtro" prefHeight="25.0" prefWidth="201.0" promptText="Filtro" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
         </top>
         <bottom>
            <HBox alignment="TOP_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="agregar" mnemonicParsing="false" onAction="#agregar" text="Agregar" />
                  <Button mnemonicParsing="false" onAction="#modificar" text="Modificar" />
                  <Button mnemonicParsing="false" onAction="#eliminar" text="Eliminar" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
